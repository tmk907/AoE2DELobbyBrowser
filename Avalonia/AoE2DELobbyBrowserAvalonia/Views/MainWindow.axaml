<Window
    x:Class="AoE2DELobbyBrowserAvalonia.Views.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="using:AoE2DELobbyBrowserAvalonia.Views"
    xmlns:vm="using:AoE2DELobbyBrowserAvalonia.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:MainWindowViewModel"
    Icon="/Assets/appIcon.ico"
    mc:Ignorable="d">
    <TransitioningContentControl Content="{Binding ContentViewModel}" PageTransition="{Binding PageTransition}" />
</Window>
