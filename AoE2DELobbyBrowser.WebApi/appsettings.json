{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log.txt",
          "rollingInterval": "Day",
          "buffered": "false"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "CacheExpiration": 5,
  "SteamWebApiKey": ""
}
