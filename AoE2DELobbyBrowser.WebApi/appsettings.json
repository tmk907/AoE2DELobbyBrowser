{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Seq", "Serilog.Enrichers.ClientInfo" ],
    "WriteTo": {
      "SeqSink": {
        "Name": "Seq",
        "Args": {
          "apiKey": "",
          "ServerUrl": "http://seq:5341"
        }
      }
    },
    "Enrich": [
      "WithClientIp"
    ]
  },
  "AllowedHosts": "*",
  "RefreshInterval": 4,
  "CacheExpiration": 5,
  "SteamWebApiKey": "",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=./data/aoe2lobby.db"
  }
}
